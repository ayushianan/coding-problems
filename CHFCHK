#include <iostream> 
#include <algorithm> 
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int arr[n];
	    int min=100000;
	    for(int i=0;i<n;i++)
	    {
	        cin>>arr[i];
	        //cout<<arr[i];
	        if(arr[i]<min)
	            min=arr[i];
	    }
	    cout<<min<<"\n";
	}
	return 0;
}


