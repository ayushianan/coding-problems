#include <iostream>
using namespace std;

int main() {
    long int t;
    cin>>t;
    while(t--)
    {
        long int n,k;
        cin>>n>>k;
        if(k==1)
            cout<<"NO";
        else if(n==k)
            cout<<"YES";
        else if((n/k)%k==0)
            cout<<"NO";
        else 
            cout<<"YES";
        cout<<"\n";
    }
	// your code goes here
	return 0;
}

