#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int r,c;
	    cin>>r>>c;
	    if(r==1&&c==1)
	    {
	        cout<<25<<"\n";
	        cout<<2<<" "<<2<<"\n"<<3<<" "<<1<<"\n"<<1<<" "<<3<<"\n"<<2<<" "<<2<<"\n"<<3<<" "<<3<<"\n"<<5<<" "<<1<<"\n"<<1<<" "<<5<<"\n"<<3<<" "<<3<<"\n"<<4<<" "<<4<<"\n"<<7<<" "<<1<<"\n"<<1<<" "<<7<<"\n"<<4<<" "<<4<<"\n"<<5<<" "<<5<<"\n"<<8<<" "<<2<<"\n"<<2<<" "<<8<<"\n"<<5<<" "<<5<<"\n"<<6<<" "<<6<<"\n"<<8<<" "<<4<<"\n"<<4<<" "<<8<<"\n"<<6<<" "<<6<<"\n"<<7<<" "<<7<<"\n"<<8<<" "<<6<<"\n"<<6<<" "<<8<<"\n"<<7<<" "<<7<<"\n"<<8<<" "<<8<<"\n";
	    }
	    else if(r==c)
	    {
	        cout<<26<<"\n";
	        cout<<1<<" "<<1<<"\n"<<2<<" "<<2<<"\n"<<3<<" "<<1<<"\n"<<1<<" "<<3<<"\n"<<2<<" "<<2<<"\n"<<3<<" "<<3<<"\n"<<5<<" "<<1<<"\n"<<1<<" "<<5<<"\n"<<3<<" "<<3<<"\n"<<4<<" "<<4<<"\n"<<7<<" "<<1<<"\n"<<1<<" "<<7<<"\n"<<4<<" "<<4<<"\n"<<5<<" "<<5<<"\n"<<8<<" "<<2<<"\n"<<2<<" "<<8<<"\n"<<5<<" "<<5<<"\n"<<6<<" "<<6<<"\n"<<8<<" "<<4<<"\n"<<4<<" "<<8<<"\n"<<6<<" "<<6<<"\n"<<7<<" "<<7<<"\n"<<8<<" "<<6<<"\n"<<6<<" "<<8<<"\n"<<7<<" "<<7<<"\n"<<8<<" "<<8<<"\n";
	    }
	    else
	    {
	        int res=(r+c)/2;
	        cout<<27<<"\n";
	        cout<<res<<" "<<res<<"\n"<<1<<" "<<1<<"\n"<<2<<" "<<2<<"\n"<<3<<" "<<1<<"\n"<<1<<" "<<3<<"\n"<<2<<" "<<2<<"\n"<<3<<" "<<3<<"\n"<<5<<" "<<1<<"\n"<<1<<" "<<5<<"\n"<<3<<" "<<3<<"\n"<<4<<" "<<4<<"\n"<<7<<" "<<1<<"\n"<<1<<" "<<7<<"\n"<<4<<" "<<4<<"\n"<<5<<" "<<5<<"\n"<<8<<" "<<2<<"\n"<<2<<" "<<8<<"\n"<<5<<" "<<5<<"\n"<<6<<" "<<6<<"\n"<<8<<" "<<4<<"\n"<<4<<" "<<8<<"\n"<<6<<" "<<6<<"\n"<<7<<" "<<7<<"\n"<<8<<" "<<6<<"\n"<<6<<" "<<8<<"\n"<<7<<" "<<7<<"\n"<<8<<" "<<8<<"\n";
	    }
	}
	return 0;
}

